<plugin name="thum_box" url="%SWFPATH%/skin/skin7/thumbspanel_bg.png" keep="true" 
	preload="false"
	align="leftbottom" edge="leftbottom" x="10" y="-5" 
	enabled="true" capture="true" handcursor="true" children="true" zorder="100" alpha="1" blendmode="normal" smoothing="true" scale="1.0"   
	width="106"
	height="47"
    visible="true"
	onloaded="mul(w,get(plugin[t_box].height),-1);add(ww,get(w),+2);set(plugin[t_box].y,get(ww));"
	onclick="if(plugin[t_box].y==0,mul(w,get(plugin[t_box].height),-1);add(ww,get(w),+2);set(plugin[t_box].y,get(ww));set(plugin[c_box].y,10);set(plugin[thum_box].y,-5),set(plugin[t_box].y,0);add(y,get(plugin[t_box].height),10);add(ty,get(plugin[t_box].height),-5);set(plugin[c_box].y,get(y));set(plugin[thum_box].y,get(ty));)"
/>

